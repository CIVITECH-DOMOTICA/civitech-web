	  .flotante{
	  }
    /* Tooltip */
    .tooltip {
      display: flex;
      align-items: center;
      gap: 5px;
      background-color: #D9D9D9;
      color: #111111;
      border-radius: 20px;
      padding: 5px 15px;
      font-size: 14px;
      box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);
      transition: opacity 0.3s, visibility 0.3s;
      position: absolute;
      right: 60px; /* Aparece a la izquierda del icono */
      top: 50%;
      transform: translateY(-50%);
      white-space: nowrap;
    }
